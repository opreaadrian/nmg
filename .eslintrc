{
  "env": {
    "node": true,
    "mocha": true,
    "browser": true,
    "amd": true,
    "jquery": true,
    "jasmine": true,
    "protractor": true,
    "es6": true
  },
  "globals": {
    "socketIoEnhancementsModule": true,
    "newMemoryStorage": true,
    "track": true,
    "_": true,
    "Switchery": true,
    "io": true,
    "sinon": true
  },
  "rules": {
    "no-trailing-spaces": [1, { "skipBlankLines": false }],
    "eol-last": 1,
    "no-empty": 1,
    "no-mixed-spaces-and-tabs": 1,
    "linebreak-style": [1, "unix"],
    "no-multi-str": 1,
    "key-spacing": [1, {
      "beforeColon": false,
      "afterColon": true
    }],
    "space-unary-ops": [1, {
      "words": true,
      "nonwords": false
    }],
    "no-spaced-func": 1,
    "space-infix-ops": [1, {"int32Hint": false}],
    "keyword-spacing": [1, {"before": true, "after": true}],
    "space-before-blocks": [1, "always"],
    "space-before-function-paren": [1, "never"],
    "one-var": [1, {
      "var": "never",
      "let": "never",
      "const": "never"
    }],
    "no-shadow-restricted-names": 1,
    "no-shadow": [1, {"hoist": "all"}],
    "no-catch-shadow": [1],
    "no-irregular-whitespace": 1,
    "strict": [1, "global"],
    "no-eq-null": 1,
    "no-use-before-define": 1,
    "no-unused-vars": [1, {"vars": "all", "args": "after-used"}],
    "no-undef": 1,
    "no-undef-init": 1,
    "dot-notation": [1, {"allowKeywords": true, "allowPattern": ""}],
    "dot-location": [1, "property"],
    "no-caller": 1,
    "eqeqeq": 1,
    "quotes": [1, "single"],
    "no-mixed-requires": [0, false],
    "no-underscore-dangle": 0,
    "yoda": [1, "never", {
      "exceptRange": false,
      "onlyEquality": false
    }],
    "indent": [1, 2],
    "brace-style": [1, "1tbs"],
    "comma-style": [1, "last", {"exceptions": {"ArrayExpression": false, "ObjectExpression": true, "VariableDeclaration": true} }],
    "default-case": 1,
    "guard-for-in": 1,
    "no-floating-decimal": 1,
    "no-nested-ternary": 1,
    "no-undefined": 1,
    "no-unused-expressions": 0,
    "radix": 1,
    "wrap-iife": [1, "outside"],
    "camelcase": [1, {"properties": "always"}],
    "max-len": [1, 100, 4, {"ignoreComments": true, "ignoreUrls": true}],
    "new-cap": [1, {
      "newIsCap": true,
      "capIsNew": false
    }],
    "no-label-var": 1
  }
}
